<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <display-name>Estate_db</display-name>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <servlet><!--servlet类路径配置-->
        <servlet-name>getHouseBySchool_Servlet</servlet-name>
        <servlet-class>com.getHouseBySchool_Servlet</servlet-class>
    </servlet>

    <servlet-mapping><!--servlet类映射配置-->
        <servlet-name>getHouseBySchool_Servlet</servlet-name>
        <url-pattern>/getHouseBySchool_Servlet</url-pattern>
    </servlet-mapping>
</web-app>